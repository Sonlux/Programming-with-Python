        "lux = \"Abc@1234\"\n",
        "pattern = r\"^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&]).{8,}$\"\n",
        "if re.match(pattern, lux):\n",
        "    print(\"Strong password\")\n",
        "else:\n",
        "    print(\"Weak password\")\n"
