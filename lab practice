      "cell_type": "code",
      "source": [
        "\n",
        "def factorial(lux):\n",
        "    if lux < 0:\n",
        "        return \"Invalid input. Enter a non-negative integer.\"\n",
        "    elif lux == 0 or lux == 1:\n",
        "        return 1\n",
        "    else:\n",
        "        return lux * factorial(lux - 1)\n",
        "\n",
        "# Example usage:\n",
        "num = 5\n",
        "print(f\"Factorial of {num} is {factorial(num)}\")"
      ],
