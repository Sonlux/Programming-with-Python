        "# ðŸ”¹ Creating Sets\n",
        "lux_a = {1, 2, 3, 4, 5}\n",
        "lux_b = {4, 5, 6, 7, 8}\n",
        "print(\"Set A:\", lux_a)\n",
        "print(\"Set B:\", lux_b)\n",
        "\n",
        "# ðŸ”¹ Union\n",
        "union_lux = lux_a | lux_b\n",
        "print(\"Union of A and B:\", union_lux)\n",
        "\n",
        "# ðŸ”¹ Intersection\n",
        "intersection_set = lux_a & lux_b\n",
        "print(\"Intersection of A and B:\", intersection_set)\n",
        "\n",
        "# ðŸ”¹ Difference\n",
        "diff_a_b = lux_a - lux_b\n",
        "diff_b_a = lux_b - lux_a\n",
        "print(\"A - B:\", diff_a_b)\n",
        "print(\"B - A:\", diff_b_a)"
      ],
